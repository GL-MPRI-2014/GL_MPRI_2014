fun next_playable l = {
  var u = list_hd l;
  var acc = accessible_positions u;
  if ((unit_has_played u) || (list_empty acc)) {
    next_playable (list_tl l)
  } else { 
    u 
  }
}

move default {
  var acc = accessible_positions selected_unit;
  dijkstra_to selected_unit (list_hd acc)
}

attack default {
}

init {
}

main {
  next_playable (army_of self)
}
