
type structure = [ Block | Road | Border of string ]

type t = {
  name: string;
  ~walk_cost<ocaml default="-1">: int;
  ~roll_cost<ocaml default="-1">: int;
  ~tread_cost<ocaml default="-1">: int;
  ~swim_cost<ocaml default="-1">: int;
  ~fly_cost<ocaml default="-1">: int;
  ~density<ocaml default="0">: int;
  ~structure<ocaml default="`Block">: structure;
}

type t_list = t list

